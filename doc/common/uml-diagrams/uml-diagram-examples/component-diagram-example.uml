@startuml

skinparam interface {
  ArrowColor #A31F34  
  Bordercolor #8a8b8c
  BackgroundColor #fffff0
}
skinparam component {
  ArrowColor #A31F34  
  Bordercolor #8a8b8c
  BackgroundColor #fffff0
}
[Client]

package "Javascript packages" {
  [Some library] 
}

cloud {
  interface "Some API" as SA 
  interface REST
  SA - [S3 Bucket]
  [Some Web Service] -> [EC2 Web Server]
  REST -down- [Some Web Service]
  [Client] -down-> REST
  [Backup Script] -down-> SA : "backup data"
  [EC2 Web Server] -down-> [Backup Script]
  [EC2 Web Server] -> [Server snapshots]
  [Some library] -> [Some Web Service]

  'Database architecture
  database "PostgreSQL" {
    folder "Company Data" {
      [Some Data]
    }
  }
  [Some Data] -> [EC2 Web Server]

}

package "Archive" {
  SA -> [Data Backup]
}

@enduml
