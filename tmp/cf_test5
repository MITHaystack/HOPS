*==========================================================
*basic setup
*==========================================================

dr_win -5.e-6  5.e-6
pc_mode manual
*pc_mode multitone
pc_period 30
ion_smooth true
mbd_anchor sbd
samplers 4 abcdefgh ijklmnop qrstuvwx yzABCDEF
ref_freq 6000.0
weak_channel 0.1
pc_amp_hcode .001
* general ionosphere search parameters:
ion_npts 1
ion_win  -0.0 0.0
* set tone mask for n*100 MHz and 3090 MHz:
pc_tonemask cdejnprwBC 2 16 16 1 16 16 16 2 16 16

if station G 
pc_mode multitone
ionosphere 0.0 

if station E
*pc_mode manual
ionosphere 0.0

freqs a b c d e f g h



*==========================================================
*station sampler delays
*==========================================================
if station G
  sampler_delay_x -140  180  180  180
  sampler_delay_y -140  180  180  180

if station E
  sampler_delay_x  -20 -20  -20  -20
  sampler_delay_y  -20 -20  -20  -20

if station H
  sampler_delay_x -150 130 130 130
  sampler_delay_y -150 130 130 130

if station V
  sampler_delay_x   20   -10   20   20
  sampler_delay_y   20   -10   20   20

if station Y
  sampler_delay_x  62  59  32  71
  sampler_delay_y  62  59  32  71

if station I
  sampler_delay_x   15  -15  -45  -50
  sampler_delay_y   15  -15  -45  -50

if station S
  sampler_delay_x  82  96  84  72
  sampler_delay_y  82  96  84  72

if station T
