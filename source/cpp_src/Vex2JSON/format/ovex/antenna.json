{
    "block_name": "$ANTENNA",
    "version": "1.5",
    "start_tag": "def",
    "stop_tag": "enddef",
    "type" : "compound",
    "block_type": "primitive",
    "parameters" :
    {
        "antenna_diam" : {"type": "real", "dimension": "length"},
        "antenna_name" : {"type": "string"},
        "axis_type" :
        {
            "type": "compound",
            "parameters":
            {
                "primary_axis": {"type": "string"},
                "secondary_axis": {"type": "string"},
                "axis_orientation": {"type": "real", "dimension": "angle"}
            },
            "fields":
            [
                "primary_axis",
                "#secondary_axis",
                "#axis_orientation"
            ]
        },
        "axis_offset" : {"type" : "real", "dimension" : "length"},
        "antenna_motion" :
        {
            "type": "list_compound",
            "parameters" :
            {
                "axis_type": {"type": "string"},
                "slew_rate": {"type": "real", "dimension" : "angle/time"},
                "settling_time":  {"type": "real", "dimension" : "time"}
            },
            "fields":
            [
                "axis_type",
                "slew_rate",
                "settling_time"
            ]
        },
        "pointing_sector" :
        {
            "type": "list_compound",
            "parameters" :
            {
                "sector_id": {"type": "link"},
                "axis1_type": {"type": "string"},
                "axis1_lower_limit": {"type": "real", "dimension": "angle"},
                "axis1_upper_limit": {"type": "real", "dimension": "angle"},
                "axis2_type": {"type": "string"},
                "axis2_lower_limit": {"type": "real", "dimension": "angle"},
                "axis2_upper_limit": {"type": "real", "dimension": "angle"}
            },
            "fields":
            [
                "sector_id",
                "axis1_type",
                "axis1_lower_limit",
                "axis1_upper_limit",
                "#axis2_type",
                "#axis2_lower_limit",
                "#axis2_upper_limit"
            ]
        }
    },
    "fields":
    [
        "antenna_diam",
        "antenna_name",
        "axis_type",
        "axis_offset",
        "#antenna_motion",
        "#pointing_sector"
    ]
}